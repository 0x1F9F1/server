select * from t1 tbl1, t1 tbl2 where tbl1.a=tbl2.a and tbl1.b < 3 and tbl2.b < 5;
EXPLAIN
{
  "query_block": {
    "select_id": 1,
    "table": {
      "table_name": "tbl1",
      "access_type": "ALL",
      "rows": 100,
      "filtered": 100,
      "attached_condition": "(tbl1.b < 3)"
    },
