      "table": {
        "table_name": "tbl2",
        "access_type": "ALL",
        "rows": 100,
        "filtered": 100,
        "attached_condition": "(tbl2.b < 5)"
      },
      "buffer_type": "flat",
      "join_type": "BNL",
      "attached_condition": "(tbl2.a = tbl1.a)"
